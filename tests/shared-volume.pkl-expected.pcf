examples {
  ["shared volume quadlet"] {
    """
    [Unit]
    Description=Shared Application Volume
    After=local-fs.target
    
    [Install]
    WantedBy=multi-user.target
    
    [Volume]
    VolumeName=app-shared
    Driver=local
    User=1000
    Group=1000
    Label=app=shared
    Label=tier=storage
    Options=type=tmpfs
    Options=tmpfs-size=1G
    
    """
  }
}

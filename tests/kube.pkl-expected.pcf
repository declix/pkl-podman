examples {
  ["full kube quadlet"] {
    """
    [Unit]
    Description=Test Kubernetes Deployment
    After=network.target
    
    [Install]
    WantedBy=multi-user.target
    
    [Kube]
    Yaml=/app/k8s/test.yaml
    AutoUpdate=registry
    ConfigMap=/app/k8s/config.yaml
    ConfigMap=/app/k8s/secrets.yaml
    Network=test-net
    PublishPort=8080:80
    PublishPort=9090:9090
    ExitCodePropagation=any
    GlobalArgs=--log-level=debug
    KubeDownForce=yes
    WorkingDirectory=/app/k8s
    UserNS=auto
    LogDriver=journald
    SetWorkingDirectory=/app
    PodmanArgs=--uidmap=0:100000:65536
    PodmanArgs=--gidmap=0:100000:65536
    PodmanArgs=--security-opt=label:disable=false
    PodmanArgs=--security-opt=label:type=container_t
    PodmanArgs=--volume=/var/log/test:/var/log:Z
    PodmanArgs=--volume=/data:/data:Z
    PodmanArgs=--annotation=test.key=test.value
    PodmanArgs=--log-level=info
    PodmanArgs=--replace
    PodmanArgs=--start
    PodmanArgs=--containers-conf-module=/etc/containers/test.conf
    
    """
  }
  ["simple kube quadlet"] {
    """
    [Kube]
    Yaml=/simple/app.yaml
    
    """
  }
}

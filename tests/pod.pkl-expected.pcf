examples {
  ["full pod quadlet"] {
    """
    [Unit]
    Description=Test Pod
    After=network.target
    
    [Install]
    WantedBy=multi-user.target
    
    [Pod]
    PodName=test-pod
    Network=bridge
    PublishPort=8080:80
    PublishPort=8443:443
    Dns=8.8.8.8
    Dns=8.8.4.4
    DnsOption=ndots:2
    DnsSearch=example.com
    Volume=/var/log/test:/var/log:Z
    UserNS=keep-id
    GidMap=0:100000:65536
    UidMap=0:100000:65536
    NetworkAlias=test-alias
    AddHost=host1:192.168.1.1
    PodmanArgs=--log-level=debug
    PodmanArgs=--cgroup-manager=systemd
    PodmanArgs=--hostname=test.local
    PodmanArgs=--exit-policy=stop
    PodmanArgs=--security-opt=label:disable=false
    PodmanArgs=--security-opt=label:type=container_t
    PodmanArgs=--label=app=test
    PodmanArgs=--label=environment=testing
    PodmanArgs=--annotation=test.key=test.value
    PodmanArgs=--share=net
    PodmanArgs=--share=ipc
    PodmanArgs=--infra-image=registry.redhat.io/ubi8/pause:latest
    
    """
  }
  ["simple pod quadlet"] {
    """
    [Pod]
    PodName=simple-pod
    
    """
  }
}
